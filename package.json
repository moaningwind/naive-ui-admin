{
  "name": "naive-ui-admin",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "vue3",
    "typescript",
    "naive-ui",
    "admin"
  ],
  "license": "MIT",
  "scripts": {
    "serve": "pnpm run dev",
    "dev": "vite",
    "build": "vite build && esno ./scripts/postBuild.ts",
    "build:no-cache": "pnpm clean:cache && pnpm run build",
    "report": "cross-env REPORT=true pnpm run build",
    "prepare": "husky install",
    "preview": "pnpm run build && vite preview",
    "preview:dist": "vite preview",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "typecheck": "tsc --noEmit",
    "lint:eslint": "eslint \"{build,mock,src}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{css,less,html,json,md}\"",
    "lint-staged": "lint-staged",
    "lint:pretty": "pretty-quick --staged",
    "svgo": "svgo -f src/assets/svg",
    "test": "vitest --config ./vitest.config.ts",
    "test prod gzip": "npx http-server dist --cors --gzip -c-1"
  },
  "dependencies": {
    "@vicons/antd": "^0.10.0",
    "@vicons/ionicons5": "^0.10.0",
    "@vueup/vue-quill": "^1.0.0-beta.8",
    "@vueuse/core": "^8.9.1",
    "axios": "^0.21.4",
    "blueimp-md5": "^2.19.0",
    "dayjs": "^1.11.3",
    "element-resize-detector": "^1.2.4",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "naive-ui": "^2.31.0",
    "pinia": "^2.0.14",
    "qs": "^6.11.0",
    "vfonts": "^0.1.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^15.14.9",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.37",
    "autoprefixer": "^10.4.7",
    "dotenv": "^10.0.0",
    "eslint": "^8.18.0",
    "esno": "^0.7.3",
    "husky": "^7.0.4",
    "less": "^4.1.3",
    "lint-staged": "^12.5.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "svgo": "^2.8.0",
    "typescript": "^4.7.4",
    "unocss": "^0.42.0",
    "unplugin-auto-import": "^0.9.2",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^2.9.13",
    "vite-plugin-compression": "^0.3.6",
    "vite-plugin-html": "^2.1.2",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "^1.4.1",
    "vitest": "^0.16.0",
    "webpack": "^5.73.0"
  },
  "lint-staged": {
    "*.{vue, ts, tsx}": "eslint --fix",
    "*.{html, css, less, json, md}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "engines": {
    "node": "^12 || >=14"
  }
}
